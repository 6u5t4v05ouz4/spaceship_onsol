{
  "name": "space-crypto-miner",
  "version": "1.0.0",
  "description": "Space Crypto Miner Game",
  "main": "src/game-only.js",
  "type": "module",
  "scripts": {
    "dev": "vite --open /",
    "dev-custom": "node dev-server.cjs",
    "dev-vite": "vite --open /game.html",
    "dev-login": "vite --open /login",
    "build": "vite build",
    "preview": "vite preview",
    "vercel-build": "npm run build",
    "start": "cd server && node server.js",
    "test-load": "cd server && node test-load.js",
    "test-load:small": "cd server && node test-load.js --players 50 --duration 30",
    "test-load:medium": "cd server && node test-load.js --players 100 --duration 60",
    "test-load:large": "cd server && node test-load.js --players 200 --duration 120"
  },
  "keywords": [
    "game",
    "phaser",
    "crypto"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.75.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bull": "^4.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "os-utils": "^0.0.3",
    "page": "^1.11.6",
    "pg": "^8.16.3",
    "phaser": "^3.90.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "vite": "^4.5.3"
  }
}
